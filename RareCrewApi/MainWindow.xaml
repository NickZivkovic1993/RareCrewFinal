<Window x:Class="MainWindowNN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListView x:Name="radniciListView">
            
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Name" DisplayMemberBinding="{Binding EmployeeName}" />
                    <GridViewColumn Header="Total Hours Worked" DisplayMemberBinding="{Binding TotalHoursWorked}" />
                    
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>